<%include head.ejs%>
<body >
	<%include nav.ejs%>
	
	
		<div class="container" style="padding: 100px;">
		<h1>This is carrt with user id= <%=data.userId%></h1>
		<p>
				<% for(var i=0; i<data.arrayItem.length; i++){ %>
					<script> 
					var html='';
					html+='<h3 ><%= data.arrayItem[i].id %></h3>';
					html+='<br>';
					html+='<%= data.arrayItem[i].itemName %>';
					html+='<br>';
					html+='Số lượng'+'<%=data.arrayItem[i].quantity%>';
					html+='<button data-idIncart="<%= data.arrayItem[i].id %>"class="delFromCart">Xóa</button>';
					html+='<br>';

					document.write(html);
					</script>
				<%}%>
		</p>
		</div>
		

			

	<%include foot.ejs%>
</body >
</html>